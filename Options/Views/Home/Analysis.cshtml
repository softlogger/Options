@model Options.Models.ViewModel

<div class="form-group row">
    <div class="col-md-4">
        <div>
            <p></p>
            <input id="ticker" placeholder="enter ticker" />
            <button id="searchTickerBtn" class="btn btn-primary" onclick="searchTicker()">Search <span class="glyphicon glyphicon-search"></span></button>
        </div>
    </div>

    <div class="col-md-8">
        <p id="headerId" class="form-control-static"> </p>
        <input id="askId" type="hidden" />
        <input id="lastPriceId" type="hidden" />
    </div>
</div>

@{
    if (Model == null)
    { return; }
}

<div>
    <label>Expiration Date:</label>
    <select id="expDateId"></select>

    <label>Strike:</label>
    <select id="strikeId"></select>


</div>


<div class="form-group row">
    <div class="col-md-8">
        <table id="quoteTableId" class="table table-bordered table-condensed"></table>
    </div>
    <div class="col-md-4">
        <table id="dividendTableId" class="table table-bordered table-condensed">

            <thead>
                <tr>
                    <th>Dividend Ex Date</th>
                    <th>Dividend</th>
                    <th>Periods</th>
                    <th>Total</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td id="divExDateId">N/A</td>
                    <td id="divAmountId">N/A</td>
                    <td id="divPeriodId">N/A</td>
                    <td id="divTotalId">N/A</td>

                </tr>
            </tbody>
        </table>
    </div>
</div>

@*<div>
        <label><strong>Low Prices</strong></label>
        <table id="historicalPricessId" class="table table-bordered table-condensed table-hover col-sm-push-6"></table>
    </div>*@


<div class="row">
    <div class="col-md-9">
        <table id="statementTableId" spellcheck="false" class="table table-bordered table-condensed table-striped table-hover"></table>
    </div>

    <div style="padding-top:20px" class="col-md-2">
        <a class="btn btn-danger btn-block" disabled><span id="lossRateId" class="badge"></span></a>
    </div>

    <div style="padding-top:20px" class="col-md-2">
        <a class="btn btn-warning btn-block" disabled><span id="rorId" class="badge"></span></a>
    </div>

    <div style="padding-top:20px" class="col-md-2">
        <a class="btn btn-success btn-block" disabled><span id="maxRateId" class="badge"></span></a>
    </div>


    <div style="padding-top:100px" class="col-md-2">
        <button id="resetId" class="btn btn-primary btn-block">Reset</button>
    </div>

    <div style="padding-top:10px" class="col-md-2">
        <button id="recalculateId" class="btn btn-primary btn-block">Recalculate</button>
    </div>

</div>

